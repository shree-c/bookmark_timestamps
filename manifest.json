{
    "name": "ty",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "scripting",
        "activeTab"
    ],
    "description": "Test",
    "host_permissions": [
        "https://youtube.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://youtube.com/*"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ]
}